{
    "family": "JMeter",
    "executionRoleArn": "arn:aws:iam::134633749276:role/test-stack-1-EC2ResourceAccessRole-1KA5P0AG92ST4",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "JMeter-container",
            "image": "134633749276.dkr.ecr.us-east-2.amazonaws.com/jmeter_client:latest",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "HOST_NAME",
                    "value": "ecs-load-balancer-f168440c517f74e4.elb.us-east-2.amazonaws.com"
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "efs-test",
                    "containerPath": "/results"
                }
            ]
        }
    ],
    "volumes": [
        {
            "name": "efs-test",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-932392eb",
                "rootDirectory": "/"
            }
        }
    ],
    
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "2048",
    "memory": "4096",
    "tags": [
        {
            "key": "User",
            "value": "daksith@wso2.com"
        }
    ]
}